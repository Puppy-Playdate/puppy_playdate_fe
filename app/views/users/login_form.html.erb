<%= form_with url: login_path, method: :post do |f| %>
  <%= f.label :email, 'Email:' %>
  <%= f.text_field :email %>

  <%= f.label :full_name, 'Full Name:' %>
  <%= f.text_field :full_name %>

  <%= f.label :password, 'Password:' %>
  <%= f. text_field :password %>

  <br><%= f.submit 'Login' %>
<% end %>

<br>
<%= button_to "Login with Google"%>
